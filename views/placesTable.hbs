<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            <tr>
                <th>#</th>
                <th>Наименование</th>
                <th>Дата осмотра</th>
                <th>Описание</th>
            </tr>
        </thead>
        <tbody>
            {{#each places}}
            <tr>
                <td>{{this.id}}</td>
                <td>{{this.name}}</td>
                <td>{{this.insepctionDate}}</td>
                <td>{{this.description}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>